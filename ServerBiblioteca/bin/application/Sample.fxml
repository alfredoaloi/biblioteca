            <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="403.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="terminaServerButton" disable="true" mnemonicParsing="false" onAction="#terminaServer" text="Termina Server" />
                  <Button fx:id="avviaServerButton" mnemonicParsing="false" onAction="#avviaServer" text="Avvia Server" />
