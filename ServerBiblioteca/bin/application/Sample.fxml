<AnchorPane prefHeight="447.0" prefWidth="559.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <children>
      <BorderPane prefHeight="345.0" prefWidth="382.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="320.0" prefWidth="382.0" BorderPane.alignment="CENTER">
               <content>
                  <AnchorPane prefHeight="320.0" prefWidth="382.0">
                     <children>
                        <TextArea fx:id="logTextArea" editable="false" prefHeight="320.0" prefWidth="382.0" wrapText="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
